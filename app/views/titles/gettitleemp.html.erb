<h1>Listing Employees under Each title</h1>

<h3> Title </h3>
<%= @titname.title %>

<br> <br>

<table>
  <thead>
    <tr>
	  <th>Emp_no</th>
	  <th>First Name</th>
      <th>Last Name</th>
	  <th>Salary</th>
      
    </tr>
 </thead>

  <tbody>
    <% @titemp.each do |titl| %>
      <tr>
	    <td><%= titl.emp_no %></td>
        <td><%= Employee.find_by_emp_no(titl.emp_no).first_name %></td>   
	    <td><%= Employee.find_by_emp_no(titl.emp_no).last_name %></td>
		<td><%= Salarie.find_by_emp_no(titl.emp_no).salary %></td>
      </tr>
    <% end %>
	
  </tbody>
</table>